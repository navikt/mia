{
  "src_folders" : ["integration/tests"],
  "page_objects_path" : ["integration/pages"],
  "output_folder" : "../../../target/surefire-reports",
  "custom_commands_path" : "integration/commands",
  "custom_assertions_path" : "",
  "globals_path" : "./integration/globals.js",

  "selenium" : {
    "start_process" : true,
    "start_session": true,
    "server_path" : "./selenium/2.48/selenium-server-standalone-2.52.2.jar",
    "log_path" : "",
    "host" : "127.0.0.1",
    "port" : 4444,
    "cli_args" : {
      "webdriver.chrome.driver" : "./selenium/chromedriver.exe",
      "phantomjs.binary.path": "./selenium/phantomjs.exe",
      "webdriver.ie.driver" : ""
    }
  },

  "test_settings" : {
    "default" : {
      "launch_url" : "http://localhost:8486/mia",
      "selenium_port"  : 4444,
      "timeout": 5000,
      "selenium_host"  : "localhost",
      "silent": true,
      "desiredCapabilities": {
        "browserName": "chrome",
        "javaScriptEnabled": "true"
      }
    },
    "chrome" : {
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "keepAlive": false
      }
    },
    "phantomjs" : {
      "desiredCapabilities": {
        "browserName": "phantomjs",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "keepAlive": false
      }
    }
  }
}